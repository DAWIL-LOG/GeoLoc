<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/onboarding_1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:clipToOutline="true"
        android:background="@drawable/bg_app"
>
    <View
            android:id="@+id/logo_text_3"
            android:layout_width="333dp"
            android:layout_height="101dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="48dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="52dp"
            android:background="@drawable/logo_text"
    />

    <View
            android:id="@+id/rectangle_2"
            android:layout_width="435dp"
            android:layout_height="510dp"
            android:layout_alignParentLeft="true"
            android:background="@drawable/onboarding_bg"
            android:layout_alignParentBottom="true"
    />

    <ImageView android:id="@+id/onboarding1"
               android:layout_width="301dp"
               android:layout_height="225dp"
               android:layout_centerHorizontal="true"
               android:layout_alignParentTop="true"
               android:layout_marginTop="127dp"
               android:background="@drawable/onboarding1"
    />

</RelativeLayout>